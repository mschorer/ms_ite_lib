<?xml version="1.0" encoding="utf-8"?>
<mx:Window xmlns:mx="http://www.adobe.com/2006/mxml" transparent="true" systemChrome="none" backgroundColor="#FFFFFF" width="640" height="480" layout="vertical">
	<mx:VBox width="100%" verticalAlign="top" verticalGap="0" height="100%" borderStyle="none" backgroundAlpha="0.0">
	    <mx:HBox width="100%">
	      <mx:Button label="&lt;" click="browserBack( event)" id="bBrowserBack"/>
	      <mx:Button label="&gt;" click="browserFwd( event)" id="bBrowserFwd"/>
			<mx:TextInput width="100%" enter="gotoURL( event)"  borderStyle="none" borderColor="#808080" cornerRadius="0" editable="true" id="browserUrl" backgroundColor="#808080" color="#ffffff"/>
		</mx:HBox>
		<mx:HTML width="100%" height="100%" id="browser" borderStyle="inset"/>
	</mx:VBox>
</mx:Window>
